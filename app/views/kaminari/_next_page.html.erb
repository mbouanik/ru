<% if current_page.last? %>
  <li class="disabled">
    <%= content_tag :a do %>
      <i class="material-icons ">chevron_right</i>
    <% end %>
  </li>
<% else %>
  <li class="waves-effect " onClick="history.go(0)">
    <%= link_to url, :rel => 'next', :remote => remote do %>
      <i class="material-icons white-text">chevron_right</i>
    <% end %>
  </li>
<% end %>
