<div d="field-add-new">
	<% if @attendee.errors.any? %>
		<div class="message">The Form contain <%= pluralize(@attendee.errors.count, "error") %>
			<% @attendee.errors.full_messages.each do |msg| %>
				<li> <%= msg %> </li>
			<% end%>
		</div>
		<p><%= image_tag "error.gif", width: 128, loop: -1 %></p>
	<% end %>
</div>
