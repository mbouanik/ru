<div class=" white-text" id="notice">
	<%= notice %>
</div>
<div class="row">
	<div class="col s12 ">
		<div class=" col s5 search-bar white-text">
			<%= form_with url: search_path, local: true, method: :get  do |form| %>
				<%= form.text_field :q, placeholder: "Search for attendees", data: { behavior: "autocomplete" }, class: " search-bar-placeholder white-text"%>
			<% end %>
		</div>
	</div>
</div>
<br>
<br>
<div class="search-add-button paginate">
	<a class=" add-new-button large material-icons modal-trigger" href="#modal1">
		add_circle
	</a>
</div>
<div class="paginate">
	<div class="center block-attendees-pages" id="container_attendees">
		<%= render @attendees %>
	</div>
	<div class="paginate center container block-attendees-pages ">
		<%= paginate @attendees %>
	</div>
</div>
<div class="">
	<%= render 'attendees/modal' %>
</div>
