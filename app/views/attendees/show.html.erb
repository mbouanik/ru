		<%= image_tag  "https://cdn.intra.42.fr/users/medium_#{ @attendee.login }.jpg", class: 'picture-index'%>
	</div>

	<div class="container  left">
		<h5 ><strong><%= @user_quest["displayname"] %> </strong> </h5>

		<h5><strong><%= @attendee.login %> </strong> </h5>
		<!-- <br>
		<br> -->
		  <h3 class="right"><strong> <%= get_pos(@attendee.login) %></strong></h3>
		<!-- <p><strong>Location: </strong><%= @user_quest["location"] %> </p> -->
	</div>

	<div class="container center">
	<h5> <strong>Grade: </strong> <%= @user_quest["cursus_users"][0]["grade"] %>  </h5>
	<br>
		<h5> <strong>Level:</strong> <%= @user_quest["cursus_users"][0]["level"] %> </h5>
<br>
 		<h5> <strong>Correction points: </strong> <%= @user_quest["correction_point"] %> </h5>
  <!-- <strong>Last Project: </strong>
  <%= @user_quest["projects_users"][-1]['project']['name'] %>
  </p> -->
	</div>
</div>

<br>

<div class="" id="link" >
	<%= link_to 'Add TimeStamp', sign_in_path(attendee_id: @attendee.id), class: 'waves-effect waves-light btn blue darken-2', remote: true %>
	<%= link_to 'Back', attendees_path, class: 'waves-effect waves-light btn blue darken-2', onClick: "history.go(0)" %>
</div>

<div class="card z-depth-3 " >
	<div class="card-title-time card-title white-text">
			<table class=" white-text ">
				<thead class="">
					<tr>
						<th class="center" id="sign_in_title" >Sign in</th>
						<th >Sign out</th>
						<th colspan="3"></th>
					</tr>
				</thead>
			</table>
		</div>
	<div class="card-content love time-stamps" >
		<div class="truncate" id="sign_in">
			<%= render @stamps %>
		</div>
	</div>
</div>

<div class="paginate center">
	<%= paginate @stamps %>
</div>

<br>
