<%- if devise_mapping.omniauthable? %>
	<%- resource_class.omniauth_providers.each do |provider| %>
	<%= link_to "Sign in with #{OmniAuth::Utils.camelize(provider)}", omniauth_authorize_path(resource_name, provider), class: 'waves-effect waves-light btn blue darken-2' %><br />
	<% end -%>
<% end -%>
